{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "A simple TypeScript Express.js todo application with SPA frontend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"nodemon dist/server.js\"",
    "build": "tsc",
    "start": "node dist/server.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.1",
    "typeorm": "^0.3.20",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.0",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": ["todo", "express", "typescript", "spa", "ajax"],
  "author": "dodo",
  "license": "MIT"
}
